<h1>Lists</h1>
<ul>
  <% @lists.each do |list| %>
    <% if list.attached? %>
      <li><%= cl_image_tag list.photo.key %></li>
    <% else %>
      <li>No image</li>
    <% end %>
    <li><%= link_to list.name, list_path(list) %></li>
  <% end %>
</ul>
<%= link_to 'Make a new list', new_list_path(@list) %>
